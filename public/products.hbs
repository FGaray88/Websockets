<div id="product-list"style="display: flex; flex-wrap: wrap; padding: 5px; background-color: darkslategrey">
    {{#each products}}
        <div>
            <div style="height: 250px; width: 250px; margin:16px;">
                <img src={{this.picture}} alt={{this.id}} style="width: 100%; min-height: 100%; max-height: 100%; border-radius: 25px">
            </div>
            <h3 style="font-family: monospace; text-align: center; font-size:large; color:whitesmoke">{{this.nombre}}</h3>
            <h4 style="font-family: monospace; text-align: center; font-size:large; color:whitesmoke">${{this.precio}}</h4>
        </div>
    {{/each}}
</div>



